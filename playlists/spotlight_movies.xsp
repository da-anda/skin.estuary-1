<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>Recently added + inprogress</name>
    <match>one</match>
	 <rule field="playlist" operator="is">special://skin/playlists/inprogress_movies.xsp</rule>
    <rule field="playlist" operator="is">special://skin/playlists/recent_movies.xsp</rule>
    <limit>25</limit>
	 <order direction="descending">lastplayed</order>
</smartplaylist>